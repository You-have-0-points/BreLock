{
    "manifest_version": 3,
    "name": "BreLock",
    "version": "1.0",
    "description": "A simple password manager.",
    "permissions": [
        "storage",
        "activeTab",
        "scripting"
    ],
    "host_permissions": [
        "<all_urls>"
    ],
    "content_scripts": [
        {
            "matches": [ "<all_urls>" ],
            "js": [ "content.js" ],
            "run_at": "document_idle"
        }
    ],
    "icons": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
    }
}
